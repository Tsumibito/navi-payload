{
  "name": "navi-payload",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "NEXT_WEBPACK=1 next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint",
    "payload:types": "PAYLOAD_CONFIG_PATH=src/payload.config.ts payload generate:types",
    "payload:importmap": "PAYLOAD_CONFIG_PATH=src/payload.config.ts payload generate:importmap",
    "migrate:sanity": "NODE_OPTIONS=--no-experimental-fetch tsx scripts/migrate-sanity-to-payload.ts",
    "migrate:tags-localized": "tsx --env-file=.env scripts/migrate-tags-to-localized.ts",
    "migrate:team-localized": "tsx --env-file=.env scripts/migrate-team-to-localized.ts",
    "migrate:posts-localized": "tsx --env-file=.env scripts/migrate-posts-to-localized.ts",
    "migrate:certificates-localized": "tsx --env-file=.env scripts/migrate-certificates-to-localized.ts",
    "migrate:seo-fields": "tsx --env-file=.env scripts/run-migration.js"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "3.901.0",
    "@payloadcms/db-postgres": "^3.57.0",
    "@payloadcms/next": "^3.57.0",
    "@payloadcms/payload-cloud": "^3.57.0",
    "@payloadcms/richtext-lexical": "^3.57.0",
    "@payloadcms/storage-s3": "^3.57.0",
    "@portabletext/to-html": "^2.0.1",
    "@sanity/client": "^6.23.4",
    "graphql": "^16.11.0",
    "jsdom": "^26.1.0",
    "mime-types": "^2.1.35",
    "next": "15.5.4",
    "node-fetch": "^3.3.2",
    "payload": "^3.57.0",
    "react": "19.1.0",
    "react-dom": "19.1.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/jsdom": "^21.1.6",
    "@types/mime-types": "^2.1.4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "dotenv": "^17.2.3",
    "eslint": "^9",
    "eslint-config-next": "15.5.4",
    "tailwindcss": "^4",
    "tsx": "^4.19.1",
    "typescript": "^5"
  }
}
